<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.azcx9d.business.dao.BTradeDAO">


	<select id="selectAll" resultType="BTrade">
		select * from trade
	</select>

	<select id="selectById" parameterType="int" resultType="BTrade">
		select * from trade where id=#{id}
	</select>

</mapper>



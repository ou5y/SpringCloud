<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android"> 
	<item android:state_pressed="true"> 
		 <shape android:shape="rectangle">  
		 <gradient 
		   	android:startColor="#689F73"
		    android:endColor="#225E2E"    
		    android:angle="270" />   
		 <corners android:radius="3dp" />   
		 <padding 
		     android:left="10dp" 
		     android:top="10dp"   
			 android:bottom="10dp"
			 android:right="10dp"/>   
		 </shape>  
	</item>   
	<item>
		  <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">  
		  <gradient    
		      android:startColor="#225E2E"    
		      android:endColor="#689F73"    
		      android:angle="270" />    
		  <corners    
		      android:radius="3dp" />    
		  <padding    
		      android:left="10dp"    
		      android:top="10dp"    
		      android:right="10dp"    
		      android:bottom="10dp" />    
		</shape>  
	</item> 
</selector>
